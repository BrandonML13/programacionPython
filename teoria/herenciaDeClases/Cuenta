class Madre:
    def __init__(self,valor,tipo):
        self.__valor = valor
        self.__tipo = tipo
        
    def depositar(self,cantidad):
        self.__valor = self.__valor + cantidad
        
    def __str__(self):
        return f"Cuenta tipo: {self.__tipo}, Saldo disponible: ${self.__valor:.2f}"


cuenta1 = Madre(75300,"Debito")
